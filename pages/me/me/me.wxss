page {
  background: #fff;
}

.head {
  display: flex;
  align-items: center;
  flex-direction: column;
  position: relative;
  height: 380rpx; 
  background: url(data:image/jpeg;base64,);
  background-size: 100% 100%;
}


.h-cont {
  padding-top: 80rpx;
  display: flex;
  align-items: center;
  /* position: absolute; */
  width: 100%;
}

.hc-left {
  position: relative;
}

.hc-left > .img-sex {
  position: absolute;
  top: 0;
}

.hc-left > .img-head {
  background: #ddd;
  border-radius: 186rpx;
  /* overflow: hidden */
}

.h-cont > view {
  margin-left: 54rpx;
}

.h-cont-right {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-right: 76rpx;
}

.h-bottom {
  display: flex;
  align-items: center;
  margin-top: 34rpx;
  padding-left: 60rpx;
  width: 100%;
  height: 70rpx;
  display: flex;
  color: #C3A25F;
  font-size: 26rpx;
  background: rgba( 255, 255, 255, .4)
}

.h-bottom > view {
  margin-right: 65rpx;
}

.listWrap {
  padding: 0 30rpx;
}

.m-item {
  display: flex;
  justify-content: space-between;
  min-height: 170rpx;
  border-bottom: 1px solid #E5E5E5
}

.m-left {
  display: flex;
  align-items: center;
  position: relative;
}

.m-left .ml-tit {
  font-size: 28rpx;
  font-weight: bold;
}

.m-left .ml-num {
  position: absolute;
  bottom: 10rpx;
  font-size: 30rpx;
  color: #C59F42;
}

.m-img {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-right: -20rpx;
}

.m-img .mi-box {
  display: flex;
  align-items: center;
  position: relative; 
  margin-right: 20rpx;
  width: 100rpx;
  height: 100rpx;
}

.m-right {
  width: 118rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.m-img .mi-box > image {
  /* background: red */
  max-height: 100rpx
}

.m-img .mi-tag {
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  top: -10rpx;
  right: -30rpx; 
  font-size: 20rpx;
  line-height: 1;
  width: 68rpx;
  height: 28rpx;
  border-radius: 6rpx;
  color: #fff;
  background: #C3A25F;
}

.m-dot {
  display: flex;
  align-items: center;
  justify-content: center;
}

.m-dot > image {
}

.m-arrows {
  display: flex;
  align-items: center;
  justify-content: flex-end;
}

.m-arrows > image {
}

.user-Info {
  color: #333;
  font-size: 26rpx;
  border: none;
  background: transparent;
}

.user-Info::after {
  display: none
}


