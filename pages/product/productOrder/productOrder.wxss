/* pages/product/productOrder/productOrder.wxss */

.orderTitle {
  font-size: 32rpx;
  font-weight: 300;
  color: rgba(51, 51, 51, 1);
  height: 131rpx;
  line-height: 131rpx;
  text-align: center;
}

.orderAddress {
  padding-left: 32rpx;
  padding-right: 38rpx;
  /* border: 1px solid red; */
  padding-bottom: 30rpx;
  background-color: #fff;
}

.orderAddress .selectAdd {
  display: flex;
  align-items: center;
  font-size: 28rpx;
  font-weight: 400;
  height: 119rpx;
  border-bottom: 1px solid rgba(228, 228, 228, 1);
}

.orderAddress .take {
  margin-left: 110rpx;
}

.orderAddress .noSupport {
  display: flex;
  align-items: center;
  margin-left: 17rpx;
  font-size: 22rpx;
  font-weight: 300;
  color: rgba(102, 102, 102, 1);
}

.orderAddress .search_icon {
  vertical-align: middle;
}

.contactAddr {
  margin-top: 42rpx;
  position: relative;
}

.contactAddr .contacts {
  display: flex;
  margin-left: 109rpx;
  align-items: center;
}

.contactAddr .name {
  font-size: 32rpx;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.contactAddr .tel {
  font-size: 32rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  margin: 0 20rpx;
}

.contactAddr .default {
  width: 76rpx;
  line-height: 30rpx;
  background: rgba(192, 158, 87, 1);
  font-size: 22rpx;
  font-family: PingFangSC-Light;
  font-weight: 300;
  color: rgba(51, 51, 51, 1);
  text-align: center;
}

.contactAddr .addrDetail {
  margin-top: 21rpx;
  margin-left: 109rpx;
  margin-right: 85rpx;
  font-size: 26rpx;
  font-weight: 300;
  color: rgba(102, 102, 102, 1);
  line-height: 49rpx;
}

.contactAddr .loactionIcon {
  position: absolute;
  top: 0;
  left: 0;
}

.contactAddr .arrowIcon {
  width: 16rpx;
  height: 26rpx;
  position: absolute;
  top: 45rpx;
  right: 0;
}

.orderDetail {
  padding: 0rpx 38rpx;
  background-color: #fff;
  margin-top: 26rpx;
}

.orderDetail .productPic {
  width: 140rpx;
  height: 140rpx;
}

.orderDetail .header {
  display: flex;
  align-items: center;
  border-bottom: 1px solid #e4e4e4;
  padding-top: 30rpx; 
  padding-bottom: 20rpx;
}

.orderDetail .picDes {
  margin-left: 23rpx;
}

.orderDetail .productDes {
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 415rpx;
  margin-left: 101rpx;
}

.orderDetail .productDes .detailDes {
  font-size: 26rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.orderDetail .productDes .format {
  font-size: 24rpx;
  font-weight: 300;
  color: rgba(153, 153, 153, 1);
  display: flex;
  margin-top: 14rpx;
  margin-bottom: 34rpx;
}

.orderDetail .productDes .size {
  margin-left: 50rpx;
}

.orderDetail .number {
  display: flex;
  justify-content: space-between;
}

.orderDetail .number .price {
  font-size: 32rpx;
  font-weight: normal;
  color: rgba(0, 0, 0, 1);
}

.orderDetail .number .btnStyle {
  display: flex;
  margin-left: 53rpx;
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(172, 172, 172, 1);
  text-align: center;
  line-height: 53rpx;
}

.orderDetail .btnStyle .minu {
  width: 59rpx;
  height: 53rpx;
  border: 1rpx solid rgba(192, 192, 192, 1);
  border-top-left-radius: 14rpx;
  border-bottom-left-radius: 14rpx;
}

.orderDetail .btnStyle .value {
  width: 59rpx;
  height: 53rpx;
  border: 1rpx solid rgba(192, 192, 192, 1);
  margin-left: 2rpx;
  margin-right: 3rpx;
  color: #474747;
}

.orderDetail .btnStyle .minu:active, .orderDetail .btnStyle .add:active {
  color: #C1A15C;
}

.orderDetail .btnStyle .add {
  width: 59rpx;
  height: 53rpx;
  border: 1rpx solid rgba(192, 192, 192, 1);
  border-top-right-radius: 14rpx;
  border-bottom-right-radius: 14rpx;
}

.orderDetail .middle {
  height: 98rpx;
  font-size: 24rpx;
  font-weight: 300;
  color: rgba(153, 153, 153, 1);
  line-height: 98rpx;
  border-bottom: 1px solid #e4e4e4;
}

.orderDetail .footer {
  display: flex;
  margin-top: 48rpx;
}

.orderDetail .footer .noteTile {
  font-size: 24rpx;
  font-weight: 400;
  margin-right: 15rpx;
  color: rgba(51, 51, 51, 1);
}

.orderDetail .footer textarea {
  box-sizing: border-box;
  padding: 10rpx;
  width: 520rpx;
  height: 128rpx;
  border: 1px solid rgba(186, 186, 186, 1);
}

.orderDetail .footer .footerDes {
  font-size: 24rpx;
  color: #999;
  text-align: right;
}

.payTitle {
  height: 131rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Light;
  color: rgba(51, 51, 51, 1);
  line-height: 131rpx;
  text-align: center;
}

.pay {
  height: 606rpx;
  background-color: #fff;
  padding: 44rpx 38rpx 0 38rpx;
}

.pay .supportWrap {
  height: 113rpx;
  border-bottom: 1rpx solid #e4e4e4;
}

.pay .support {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.pay .noSupport {
  font-size: 22rpx;
  font-weight: 300;
  color: rgba(153, 153, 153, 1);
  margin-top: 10rpx;
}

.pay .wxConfirm {
  display: flex;
  height: 178rpx;
  align-items: center;
  border-bottom: 1px solid #e4e4e4;
}

.pay .wxConfirm .logo {
  width: 96rpx;
  height: 79rpx;
  margin-left: 21rpx;
}

.pay .wxConfirm .wxDes {
  margin-left: 79rpx;
}

.pay .wxConfirm .first {
  font-size: 32rpx;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  height: 50rpx;
}

.pay .wxConfirm .second {
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  height: 50rpx;
}

.pay .wxConfirm .circle {
  margin-left: 260rpx;
  width: 39rpx;
  height: 39rpx;
  border: 1rpx solid #e4e4e4;
  border-radius: 50%;
  overflow: hidden;
}

.pay .wxConfirm .circle image {
  width: 39rpx;
  height: 39rpx;
}

.money {
  margin-top: 36rpx;
  overflow: hidden;
}

.money .centent {
  float: right;
}

.money .centent .ul {
  display: flex;
  justify-content: space-between;
  margin-top: 11rpx;
  margin-bottom: 11rpx;
}

.money .centent .ul .title {
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  margin-right: 57rpx;
}

.money .centent .ul .text {
  font-size: 32rpx;
  font-weight: normal;
  color: rgba(102, 102, 102, 1);
}

.money .centent .ul .total {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.money .centent .ul .totalMoney {
  font-size: 38rpx;
  font-weight: normal;
  color: rgba(0, 0, 0, 1);
}

.foot {
  width: 100%;
  position: fixed;
  bottom: 0;
  margin-top: 52rpx;
  height: 100rpx;
  display: flex;
}

.foot .left {
  width: 518rpx;
  height: 100rpx;
  background: rgba(71, 71, 71, 1);
  padding-left: 39rpx;
  box-sizing: border-box;
}

.foot .right {
  width: 232rpx;
  height: 100rpx;
  background: rgba(192, 158, 87, 1);
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(37, 37, 34, 1);
  line-height: 100rpx;
  text-align: center;
}

.foot .left .computWrap {
  display: flex;
  align-items: center;
}

.foot .left .comput {
  font-size: 32rpx;
  font-weight: 300;
  color: #fff;
}

.foot .left .computMoney {
  font-size: 40rpx;
  font-weight: normal;
  color: rgba(192, 158, 87, 1);
}

.foot .left .Number {
  margin-top: 10rpx;
  font-size: 20rpx;
  font-weight: 300;
  color: rgba(202, 202, 202, 1);
}

/* 没有默认地址 */
.noddr {

}

.noddr .nd-top {
  margin: 40rpx 0;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  color: #999
}

.noddr .nd-top > image {
  margin-right: 20rpx;
}

.noaddr-btn {
  width: 264rpx;
  height: 72rpx;
  color: #fff;
  background: #C09E57
}